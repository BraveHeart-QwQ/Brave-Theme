    "tokenColors": [
        {
            "name": "Debug Color (Use to track some scope)",
            "scope": [
                "tag",
                "unit",
                "definition",
                "anonymous",
            ],
            "settings": {
                "foreground": "#24292f",
                "fontStyle": "underline strikethrough"
            }
        },
        /* ---------------- Main Color ---------------- */
        {
            "name": "Default",
            "scope": [
                "none",
                /* --- 符号 --- */
                "punctuation.definition",
                "keyword.operator",
                "bracket",
                "comma",
                "punctuation",
                "separator",
                "customrule",
                "brace",
                "square",
                "terminator",
                /* --- 其他 --- */
                "variable.parameter",
                "variable.other.local",
                "variable.other.property",
                /* --- 语言限定 --- */
                "meta.preprocessor.cs",          // C# 预处理器
                "source.cs variable.other.readwrite",
                "source.cs variable.other.object",
                "source.python constant.other", // 模块变量...
                "diagram.source.wsd support.variable", // Plantuml
                "diagram.source.wsd string.quoted",
                "variable.other.readwrite", // JS
                "variable.other.object.js", // JS
                "storage.type.function.arrow.ts", // TS
                "meta.function-call.arguments", // python 里的调用
                "variable.other.lua",  // Lua 变量
                "entity.name.tag.html invalid.illegal.unrecognized-tag.html", // HTML 非标准标签
            ],
            "settings": {
                "foreground": "#24292f",
                "fontStyle": "",
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.comment",
                "punctuation.definition.comment", // 注释符号
                "storage.type.class.doxygen",
                "comment variable.parameter",
                "source.cpp storage.type.modifier.access.control", // 灰化访问控制符 
                "source.cpp punctuation.separator.colon.access.control", // 灰化访问控制符 

                "meta.preprocessor.cs keyword",  // C# 预处理器
                "meta.preprocessor.cs punctuation",  // C# region
                "meta.preprocessor.cs keyword.preprocessor.region",  // C# region
                "meta.preprocessor.cs keyword.preprocessor.endregion",  // C# region
                "meta.preprocessor.cs string",  // C# region
            ],
            "settings": {
                "foreground": "#A0A1A7",
            }
        },
        {
            "name": "Type",
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class", // Python 里继承
                "embedded",
                "source.python meta.function.parameters constant.other.caps", // Python 形参约束类型
                "source.python support.type.exception", // Python 形参约束类型
                "support.class", // JS
            ],
            "settings": {
                "foreground": "#C18401"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "support",
                "storage",
                "storage.type.annotation",
                "storage.type.primitive",
                "keyword",
                "keyword.operator.expression",  // 如：C# 的 new 关键字
                // "keyword.control",
                // "keyword.operator.logical",
                // "keyword.operator.instanceof",
                // "keyword.operator.delete",
                // "keyword.operator.in",
                // "keyword.operator.of",
                // "keyword.operator.instanceof",
                // "keyword.operator.new",
                // "keyword.operator.typeof",
                // "keyword.operator.void",
                // "keyword.operator.expression",
                "selector",
                "punctuation.definition.directive", // #define 中的 #
            ],
            "settings": {
                "foreground": "#A626A4"
            }
        },
        {
            "name": "Variable",
            "scope": [
                "variable",
                "property",
                "property-name",
                "property-value",
                "attribute",
                "attribute-name",
                "variable.language", // JS 的 this
            ],
            "settings": {
                "foreground": "#E45649"
            }
        },
        {
            "name": "Method",
            "scope": [
                "require",
                "entity.name.function",
                "special-method",
                "any-method",
                "preprocessor",
                "entity.name.section", // Markdown 标题
                "string.other.link.title", // Markdown 链接标题
                "punctuation.definition.heading.markdown", // Markdown 标题符号
                "source.python meta.function-call.generic", // 函数调用
                "source.python support.function", // 如：len(), __init__()
                "meta.function-call", // python 里的调用
                "entity.name.variable.property.cs",  // C# 属性
                "source.lua support.function",  // Lua 函数
                "support.function.lua",  // Lua 函数
                "source.lua support.function.any-method.lua",
            ],
            "settings": {
                "foreground": "#4078F2"
            }
        },
        {
            "name": "String",
            "scope": [
                "quoted",
                "string",
                "punctuation.definition.char", // 字符串引号
                "punctuation.definition.string", // 字符串引号
            ],
            "settings": {
                "foreground": "#50A14F"
            }
        },
        {
            "name": "Special String",
            "scope": [
                "escape",
                "support.other.escape",
                "constant.character.escape",
                "color",
                "symbol",
                "regexp",
                "language",
            ],
            "settings": {
                "foreground": "#0184BC"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "bool",
                "constant",
                "punctuation.definition.constant",
                "variable.other.enummember",
                "source.python variable.parameter", // Python 参数变色
                "source.python variable.parameters", // Python 参数变色
                "variable.parameter.js", // JavaScript 参数变色
                "entity.name.lifetime.rust",
            ],
            "settings": {
                "foreground": "#A76300"
            }
        },
        /* ---------------- Misc Color ---------------- */
        {
            "name": "Deep Grey",
            "scope": [
                "source.elixir array",
                "source.elixir scope",
                "source.elixir section"
            ],
            "settings": {
                "foreground": "#696C77"
            }
        },
        {
            "name": "Punctuation Section Embedded",
            "scope": [
                "variable.interpolation",
                "punctuation.section.embedded",
                "source.elixir regexp.section",
                "source.elixir regexp.string",
                "meta.template.expression.js punctuation.definition",
                "meta.template.expression.ts punctuation.definition",
            ],
            "settings": {
                "foreground": "#CA1243"
            }
        },
        {
            "name": "Invalid Deprecated",
            "scope": [
                "invalid.deprecated",
                "invalid.illegal",
            ],
            "settings": {
                "foreground": "#000000",
                "fontStyle": "italic strikethrough"
            }
        },
        /* ---------------- Markup ---------------- */
        {
            "name": "Markdown Default",
            "scope": [
                "meta.embedded.math.markdown",
                "text.html.markdown invalid.deprecated"
            ],
            "settings": {
                "foreground": "#24292f",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "underline",
                "markup.underline",
            ],
            "settings": {
                "fontStyle": "underline",
            }
        },
        {
            "scope": [
                "strikethrough",
                "markup.strikethrough"
            ],
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": [
                "bold",
                "markup.bold",
                // "punctuation.definition.bold"
            ],
            "settings": {
                // "foreground": "#A76300",
                "foreground": "#CA1243",
                "fontStyle": "bold",
            }
        },
        {
            "scope": [
                "italic",
                "markup.italic",
            ],
            "settings": {
                "fontStyle": "italic",
            }
        },
        {
            "scope": [
                "markup.heading",
                "markup.heading entity.name"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0550ae"
            }
        },
        {
            "scope": [
                "markup.quote",
                "punctuation.definition.quote",
                "markup.inserted",
                "meta.diff.header.to-file",
                "punctuation.definition.inserted"
            ],
            "settings": {
                "foreground": "#116329"
            }
        },
        {
            "name": "Markdown Code",
            "scope": [
                "markup.inline.raw",
                "markup.inline.raw punctuation.definition.raw"
            ],
            "settings": {
                "foreground": "#0550ae"
            }
        },
        {
            "name": "Markdown Math",
            "scope": [
                "entity.name.function.math.tex",
                "punctuation.definition.function.math.tex",
                "constant.character.math.tex",
                "punctuation.definition.constant.math.tex",
            ],
            "settings": {
                "foreground": "#0184BC"
            }
        },
        {
            "scope": [
                "markup.deleted",
                "meta.diff.header.from-file",
                "punctuation.definition.deleted"
            ],
            "settings": {
                "foreground": "#82071e"
            }
        },
        {
            "scope": [
                "markup.changed",
                "punctuation.definition.changed",
                "punctuation.definition.list.begin.markdown",
            ],
            "settings": {
                "foreground": "#953800"
            }
        },
        {
            "scope": [
                "markup.ignored",
                "markup.untracked"
            ],
            "settings": {
                "foreground": "#eaeef2",
            }
        },
        /* ---------------- Json, XML ---------------- */
        {
            "name": "Json,XML Tag",
            "scope": [
                "source.json support.type.property-name",
                "source.json punctuation.support.type.property-name",
                "entity.name.tag", // 除此之外，还支持 Markup 中的 Tag
            ],
            "settings": {
                "foreground": "#E45649"
            }
        },
        {
            "name": "Json,XML Variable",
            "scope": [
                "text.xml entity.other.attribute-name",
                "text.html entity.other.attribute-name",

                "comment.block.documentation.cs entity.other.attribute-name",  // C# 注释文档
